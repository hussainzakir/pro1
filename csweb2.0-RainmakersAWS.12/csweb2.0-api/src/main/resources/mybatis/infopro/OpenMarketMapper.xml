<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.repsrv.csweb.core.account.openmarket.dao.OpenMarketDao">
	
	<select id="openMarketDeleteStagingData" statementType="CALLABLE">
		Call CUQOMAAI12_DELETE_AAI_DATA
		(#{request.json},
		 #{request.returnStatus,jdbcType=CHAR,mode=OUT}, 
   		 #{request.returnErrorMsg,jdbcType=CHAR,mode=OUT}
 		) 
	</select>

	<select id="openMarketProjectCreation"  statementType="CALLABLE" >
		Call CUQOMAAI07_CREATE_PROJECTID(
						#{request.json, jdbcType=CHAR, mode=IN},                                
						 #{request.projectId, jdbcType=CHAR,mode=OUT},
						 #{request.returnStatus,jdbcType=CHAR,mode=OUT},
						 #{request.returnErrorMsg,jdbcType=CHAR,mode=OUT} 
		)
	</select>
	
	<select id="openMarketSaveProjectData"  statementType="CALLABLE" >
		Call CUQOMAAI01_PARSE_AND_LOAD(
						#{request, jdbcType=CLOB, mode=IN},                                
						 #{result.errorCode,jdbcType=CHAR,mode=OUT},
						 #{result.errorMessage,jdbcType=CHAR,mode=OUT} 
		)
	</select>

	 <select id="openMarketRequestDriver" statementType="CALLABLE">
		Call CUQOMAAI03_SBM_REQUEST_DRIVER(
						#{request.json, jdbcType=CHAR, mode=IN},                                
						#{request.returnStatus,jdbcType=CHAR,mode=OUT},
						#{request.returnErrorMsg,jdbcType=CHAR,mode=OUT} 
		)
	 </select>

	<select id="getUserImportHistory" statementType="CALLABLE" 
		resultType="java.lang.String" resultMap="clobToStringMap">
		CALL CUQACQ100_CSWEB_ACTIVITIES (
		#{userId},
		'1',
		#{result.sqlState,jdbcType=CHAR,mode=OUT},
        #{result.errorMessage,jdbcType=CHAR,mode=OUT})
	</select>
	
	<select id="getErrorData" resultMap="clobToStringMap">
		CALL CUQOMGETER_GET_ERROR_RESPONSE(
			#{projectId, jdbcType=CHAR, mode=IN},
			'',
			#{request.errorCode,jdbcType=CHAR,mode=OUT},
			#{request.errorMessage,jdbcType=CHAR,mode=OUT}
			 ) 
	</select>
		
	<resultMap type="java.lang.String" id="clobToStringMap" >
		<result column="data" javaType="java.lang.String" jdbcType="CLOB" 
		typeHandler="org.apache.ibatis.type.ClobTypeHandler"/>
	</resultMap>
</mapper>